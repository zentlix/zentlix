    <link rel="apple-touch-icon" sizes="114x114" href="{{ asset('bundles/core/admin/assets/favicon/apple-icon-114x114.png') }}">
    <link rel="apple-touch-icon" sizes="120x120" href="{{ asset('bundles/core/admin/assets/favicon/apple-icon-120x120.png') }}">
    <link rel="apple-touch-icon" sizes="144x144" href="{{ asset('bundles/core/admin/assets/favicon/apple-icon-144x144.png') }}">
