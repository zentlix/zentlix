    <link href="{{ asset('bundles/core/admin/vendors/dropzone/css/dropzone.min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/core/admin/vendors/spin-kit/css/spinkit.min.css') }}" rel="stylesheet">
    {% block header_scripts %}{% endblock %}
    <script src="{{ asset('bundles/core/admin/vendors/jquery/jquery.min.js') }}"></script>
    <link href="{{ asset('bundles/core/admin/css/style.css') }}" rel="stylesheet">
</head>
<body class="c-app {% block body_class %}{% endblock %}">
    {% block content %}{% endblock %}
    <script src="{{ asset('bundles/core/admin/vendors/cke/ckeditor.js') }}"></script>
    <script src="{{ asset('bundles/core/admin/vendors/cke/lang/' ~ app.request.locale ~ '.js') }}"></script>
    <script src="{{ asset('bundles/core/admin/vendors/datatable/datatables.min.js') }}"></script>
    <script src="{{ asset('bundles/core/admin/vendors/datatable/dataTables.bootstrap4.min.js') }}"></script>
    <script src="{{ asset('bundles/core/admin/js/datatables.js') }}"></script>
    <script src="{{ asset('bundles/core/admin/vendors/@coreui/coreui/js/coreui.bundle.min.js') }}"></script>
    <!--[if IE]><!-->
    <script src="{{ asset('bundles/core/admin/vendors/@coreui/icons/js/svgxuse.min.js') }}"></script>
    <!--<![endif]-->
    <script src="{{ asset('bundles/core/admin/vendors/@coreui/chartjs/js/coreui-chartjs.bundle.js') }}"></script>
    <script src="{{ asset('bundles/core/admin/vendors/@coreui/utils/js/coreui-utils.js') }}"></script>
    <script src="{{ asset('bundles/core/admin/vendors/toastr/toastr.min.js') }}"></script>
    <script src="{{ asset('bundles/core/admin/vendors/dropzone/js/dropzone.min.js') }}"></script>
    {% block footer_scripts %}{% endblock %}
    <script src="{{ asset('bundles/core/admin/js/main.js') }}"></script>
</body>
