    <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('bundles/core/admin/assets/favicon/favicon-32x32.png') }}">
    <link rel="icon" type="image/png" sizes="96x96" href="{{ asset('bundles/core/admin/assets/favicon/favicon-96x96.png') }}">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('bundles/core/admin/assets/favicon/favicon-16x16.png') }}">
    <link rel="manifest" href="{{ asset('bundles/core/admin/assets/favicon/manifest.json') }}">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="{{ asset('bundles/core/admin/assets/favicon/ms-icon-144x144.png') }}">
    <meta name="theme-color" content="#ffffff">
    <link href="{{ asset('bundles/core/admin/vendors/@coreui/chartjs/css/coreui-chartjs.css') }}" rel="stylesheet">
