{
    "name": "zentlix/core",
    "type": "symfony-bundle",
    "description": "Provides the basic functionality of the Zentlix CMS",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^5.4 || ^6.0",
        "doctrine/orm": "^2.12 || ^3.0",
        "doctrine/migrations": "^3.5",
        "symfony/messenger": "^5.4 || ^6.0",
        "broadway/broadway-bundle": "^0.6.1",
        "broadway/event-store-dbal": "^0.6",
        "zentlix/users": "^0.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Zentlix\\Core\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
